    "test:deploy": "powershell -ExecutionPolicy Bypass -File ./scripts/simple-deploy-test.ps1",
    "test:deploy:staging": "powershell -ExecutionPolicy Bypass -File ./scripts/simple-deploy-test.ps1 -Url https://orkut-56bknc693-julio-cesar-campos-machados-projects.vercel.app",
    "test:deploy:local": "powershell -ExecutionPolicy Bypass -File ./scripts/simple-deploy-test.ps1 -Url http://localhost:3000",
    "dev:desktop": "cd desktop && npm run dev",
    "dev:mobile": "cd mobile && npm start",
    "db:seed-photos": "node scripts/seed-photos.js",
