# Banco de dados Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google Gemini AI - Chaves específicas por função
GEMINI_API_KEY_GENERAL=your_gemini_api_key_general     # Para uso geral (Orky posts)
GEMINI_API_KEY_MUSIC=your_gemini_api_key_music         # Para análise musical (DJ Orky)
GEMINI_API_KEY_CHAT=your_gemini_api_key_chat           # Para chat/mensagens
GEMINI_API_KEY_COMMUNITY=your_gemini_api_key_community # Para comunidades

# NextAuth.js
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# Outras configurações
NODE_ENV=development
