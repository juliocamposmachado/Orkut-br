# Porta do servidor
PORT=3001

# Intervalo de atualização em minutos (padrão: 30)
UPDATE_INTERVAL_MINUTES=30

# Máximo de trending topics para retornar (padrão: 20)
MAX_TRENDS=20

# User Agent para requisições
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36

# URL do RSS do Google Trends Brasil
GOOGLE_TRENDS_RSS_URL=https://trends.google.com.br/trending/rss?geo=BR
